    <plugin name="arducopter_plugin" filename="libArduPilotPluginScaled.so">
      <fdm_addr>127.0.0.1</fdm_addr> 
      <fdm_port_in>9002</fdm_port_in>
      <fdm_port_out>9003</fdm_port_out>   
           
       <!--<listen_addr>192.168.6.1</listen_addr>
      <fdm_addr>192.168.6.2</fdm_addr> 
      <fdm_port_in>9002</fdm_port_in>      
      <fdm_port_out>9003</fdm_port_out>   --> 
      <!--
          Require by APM :
          Only change model and gazebo from XYZ to XY-Z coordinates
      -->
      <modelXYZToAirplaneXForwardZDown>0 0  0 3.141593 0 0</modelXYZToAirplaneXForwardZDown>
      <gazeboXYZToNED>0 0 0 3.141593 0 1.2</gazeboXYZToNED>
      <imuName>imu_sensor</imuName> 
      <gpsName>gpssensor</gpsName>   
      <magName>magsensor</magName> 
      <!-- <barName>barsensor</barName>  -->
        
      <connectionTimeoutMaxCount>50</connectionTimeoutMaxCount>
      
      <control channel="0">
      <!--
          incoming control command [0, 1]
          so offset it by 0 to get [0, 1]
          and divide max target by 1.
          offset = 0
          multiplier = 838 max rpm / 1 = 838
        -->
        <type>EFFORT</type>
        <offset>0</offset>
        <p_gain>0.20</p_gain>
        <i_gain>0</i_gain>
        <d_gain>0</d_gain>
        <i_max>0</i_max>
        <i_min>0</i_min>
        <cmd_max>2.5</cmd_max>
        <cmd_min>-2.5</cmd_min>
        <jointName>front_left_wheel</jointName>
        <multiplier>-100</multiplier>
        <controlVelocitySlowdownSim>1</controlVelocitySlowdownSim>
      </control>
      <control channel="1">
        <type>EFFORT</type>
        <offset>0</offset>
